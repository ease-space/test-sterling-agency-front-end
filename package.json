    "node": ">=8.9",
    "npm": ">=5.5"
  },
  "scripts": {
    "start:dev:local": "npm run clear && cross-env NODE_ENV=development BUILD_ENV=development node -- ./src/mount/main.js",
    "start:dev:server": "npm run clear && cross-env NODE_ENV=development BUILD_ENV=production node -- ./src/mount/main.js",
    "start:prod:server": "npm run clear && cross-env NODE_ENV=production BUILD_ENV=production node -- ./src/mount/main.js",
    "start": "node -- ./src/mount/server.js",
    "clear": "rimraf build/",
    "lint": "npm run lint:javascript && npm run lint:css",
    "lint:javascript": "jest --projects ./jest/jest.eslint.config.json",
    "lint:css": "jest --projects ./jest/jest.stylelint.config.json",
    "test": "jest --config ./jest/jest.test.config.json",
    "test:full": "jest --config ./jest/jest.config.json ",
    "test:watch": "npm test -- --watch",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --config ./jest/jest.test.config.json",
    "format:eslint": "prettier --write **/*.{js,jsx,json}",
    "format:stylelint": "prettier --write **/*.css",
    "format": "npm run format:eslint",
    "git:add": "git add ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run git:add",
      "pre-push": "npm run test:full"
    }
  },
